{
  "name": "cryppo-cli",
  "description": "Command line interface for Cryppo-js, a Meeco encryption library",
  "version": "2.1.0",
  "author": "Meeco",
  "bin": {
    "cryppo": "./bin/run"
  },
  "bugs": "https://github.com/Meeco/cryppo-cli/issues",
  "dependencies": {
    "@meeco/cryppo": "^2.0.0",
    "@oclif/core": "^3.19.1",
    "@oclif/plugin-help": "^6.0.12",
    "sinon": "^17.0.1",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3.1.16",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.17",
    "chai": "^4.2.0",
    "eslint": "^8.56.0",
    "eslint-config-oclif": "^5.0.0",
    "eslint-config-oclif-typescript": "^3.0.47",
    "mocha": "^10.3.0",
    "nyc": "^15.1.0",
    "oclif": "^4.4.11",
    "prettier": "^3.2.5",
    "shx": "^0.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Meeco/cryppo-cli",
  "keywords": [
    "Meeco",
    "cryptography",
    "encryption",
    "Cryppo",
    "AES",
    "AES-GCM",
    "RSA",
    "key derivation"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "cryppo",
    "dirname": "cryppo",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "macos": {
      "identifier": "me.meeco.cryppo"
    }
  },
  "repository": "Meeco/cryppo-cli",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "prepare": "yarn build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",

    "fmt:check": "prettier -c \"src/**/*.ts\" \"test/**/*.ts\"",
    "fmt:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" *.json README.md",
    "lint:fix": "eslint \"{src,test}/**/*.ts\" --fix",

    "pack": "oclif pack tarballs --no-xz --parallel",
    "pack-win": "oclif pack win",
    "pack-mac": "oclif pack macos",
    "testcov": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\""
  },
  "types": "dist/index.d.ts"
}
